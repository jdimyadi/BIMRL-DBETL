<Window x:Class="push2BIMRL.XplorerPlugin.ETLLoad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:push2BIMRL.XplorerPlugin"
        mc:Ignorable="d"
        Title="Load Model to BIMRL" Height="222.5" Width="444">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="49*"/>
            <RowDefinition Height="115*"/>
            <RowDefinition Height="31*"/>
        </Grid.RowDefinitions>
        <Button x:Name="button_Cancel" Content="Cancel" Margin="0,6,9,5" Grid.Row="2" HorizontalAlignment="Right" Width="75" Click="button_Cancel_Click"/>
        <Button x:Name="button_OK" Content="OK" Margin="0,6,89,5" Grid.Row="2" HorizontalAlignment="Right" Width="75" Click="button_OK_Click"/>
        <CheckBox x:Name="checkBox_ETLOption" Content="Run the Complete ETL Process" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top" Checked="checkBox_Checked" IsChecked="True" Height="15" Width="180"/>
        <TextBox x:Name="textBox_outputWindow" Margin="10,5,9,10" TextWrapping="Wrap" Grid.Row="1"/>

    </Grid>
</Window>
